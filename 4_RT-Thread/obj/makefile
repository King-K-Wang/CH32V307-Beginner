# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
4_RT-Thread.hex \

SECONDARY_LIST += \
4_RT-Thread.lst \

SECONDARY_SIZE += \
4_RT-Thread.siz \


# 所有目标
all: 4_RT-Thread.elf secondary-outputs

# 工具调用
4_RT-Thread.elf: $(OBJS) $(USER_OBJS)
	@	@	riscv-none-embed-gcc -march=rv32imacxw -mabi=ilp32 -msmall-data-limit=8 -msave-restore -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -Wunused -Wuninitialized  -g -T "D:\MounRiver\Projects\CH32V307-Beginner\4_RT-Thread\Ld\Link.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"4_RT-Thread.map" --specs=nano.specs --specs=nosys.specs -o "4_RT-Thread.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@	@
4_RT-Thread.hex: 4_RT-Thread.elf
	@	riscv-none-embed-objcopy -O ihex "4_RT-Thread.elf"  "4_RT-Thread.hex"
	@	@
4_RT-Thread.lst: 4_RT-Thread.elf
	@	riscv-none-embed-objdump --all-headers --demangle --disassemble -M xw "4_RT-Thread.elf" > "4_RT-Thread.lst"
	@	@
4_RT-Thread.siz: 4_RT-Thread.elf
	@	riscv-none-embed-size --format=berkeley "4_RT-Thread.elf"
	@	@
# 其他目标
clean:
	-$(RM) $(ASM_UPPER_DEPS)$(OBJS)$(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_DEPS)$(S_UPPER_DEPS)$(C_DEPS) 4_RT-Thread.elf
	-@
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

